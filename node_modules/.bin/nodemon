#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules/.registry.npmjs.org/nodemon/2.0.3/node_modules/nodemon/bin/nodemon.js/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules/.registry.npmjs.org/nodemon/2.0.3/node_modules/nodemon/bin/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules/.registry.npmjs.org/nodemon/2.0.3/node_modules/nodemon/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules/.registry.npmjs.org/nodemon/2.0.3/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules/.registry.npmjs.org/nodemon/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/temp/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/common/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/node_modules:/mnt/c/Users/sunny.redhu/Desktop/node_modules:/mnt/c/Users/sunny.redhu/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/backend/node_modules/nodemon/bin/nodemon.js/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/backend/node_modules/nodemon/bin/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/backend/node_modules/nodemon/node_modules:/mnt/c/Users/sunny.redhu/Desktop/current-stuff/phonebook-monorepo/backend/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else 
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
